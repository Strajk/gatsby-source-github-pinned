{
  "name": "gatsby-source-github-pinned",
  "description": "Gatsby source plugin for fetching pinned repositories.",
  "version": "0.0.4",
  "author": "Edgar Ramírez Mondragón <edgarrm358@gmail.com>",
  "repository": "https://github.com/EdgarRMmex/gatsby-source-github-pinned",
  "bugs": {
    "url": "https://github.com/EdgarRMmex/gatsby-source-github-pinned/issues"
  },
  "main": "index.js",
  "deprecated": false,
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "node-fetch": "^2.2.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "cross-env": "^5.2.0"
  }
}
